    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Autorouter EGKB",
            "type": "debugpy",
            "request": "launch",
            "program": "newinfo.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/euro_aip"
            },
            "cwd": "${workspaceFolder}/euro_aip/example",
            "python": "${workspaceFolder}/euro_aip/example/venv/bin/python",
            "args": ["autorouter", "EGKB"]
        },
        {
            "name": "Python Debugger: France eAIP LFAT",
            "type": "debugpy",
            "request": "launch",
            "program": "newinfo.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/euro_aip"
            },
            "cwd": "${workspaceFolder}/euro_aip/example",
            "python": "${workspaceFolder}/euro_aip/example/venv/bin/python",
            "args": ["--root-dir", "cache/eaip_17_apr_2025", "france_eaip", "LFAT"]
        }
    ]
}